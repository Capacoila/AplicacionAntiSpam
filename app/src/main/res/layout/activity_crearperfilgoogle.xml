<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Crearperfilgoogle">
    <RelativeLayout
        android:id="@+id/relativeLayout0"
        android:layout_width="match_parent"
        android:layout_height="249dp">


        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Elige una cuenta"
            android:textSize="24dp"
            android:textColor="@color/black"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"/>
        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="para continuar usando Block"
            android:textColor="@color/black"
            android:textSize="20dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_below="@id/textView10"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="380dp"
        android:layout_height="66dp"
        android:layout_below="@id/relativeLayout0"
        android:layout_centerInParent="true">

        <TextView
            android:id="@+id/textView1"
            android:layout_width="229dp"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="Jose Sanchez \n jsanchez@gmail.com"
            android:textSize="18dp" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="66dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_marginStart="17dp"
            android:layout_marginRight="7dp"
            android:layout_toLeftOf="@id/textView1"
            android:src="@drawable/usuariodeperfil_azul" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout4"
        android:layout_width="380dp"
        android:layout_height="20dp"
        android:layout_below="@id/relativeLayout1"
        android:layout_centerInParent="true"
        android:background="#FFFFFF">


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="380dp"
        android:layout_height="66dp"
        android:layout_below="@id/relativeLayout4"
        android:layout_centerInParent="true"
        >

        <TextView
            android:id="@+id/textView2"
            android:layout_width="229dp"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="Jose Sanchez Medrano \n jsmedrano@gmail.com"
            android:textSize="18dp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="66dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_marginStart="17dp"
            android:layout_marginRight="7dp"
            android:layout_toLeftOf="@id/textView2"
            android:src="@drawable/usuariodeperfil_verde" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout5"
        android:layout_width="380dp"
        android:layout_height="20dp"
        android:layout_below="@id/relativeLayout2"
        android:layout_centerInParent="true"
        android:background="#FFFFFF">


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout3"
        android:layout_width="380dp"
        android:layout_height="66dp"
        android:layout_below="@id/relativeLayout5"
        android:layout_centerInParent="true">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="229dp"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="Patty \n Patty06@gmail.com"
            android:textSize="18dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="66dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_marginStart="17dp"
            android:layout_marginRight="7dp"
            android:layout_toLeftOf="@id/textView3"
            android:src="@drawable/usuariodeperfil_rojo" />

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/relativeLayout9"
        android:layout_width="380dp"
        android:layout_height="20dp"
        android:layout_below="@id/relativeLayout3"
        android:layout_centerInParent="true"
        android:background="#FFFFFF">


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout7"
        android:layout_width="380dp"
        android:layout_height="66dp"
        android:layout_below="@id/relativeLayout9"
        android:layout_centerInParent="true">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="229dp"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="Agregar otra cuenta"
            android:textSize="18dp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="66dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_marginStart="17dp"
            android:layout_marginRight="7dp"
            android:layout_toLeftOf="@id/textView4"
            android:src="@drawable/agregar_usuario" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout6"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:layout_below="@id/relativeLayout7"
        android:layout_centerInParent="true">


        <Button
            android:id="@+id/button3"
            android:layout_width="210dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/white"
            android:text="Cancelar"
            android:textColor="@color/black"
            android:layout_centerVertical="true"/>

        <Button
            android:id="@+id/button4"
            android:layout_width="210dp"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/button3"
            android:backgroundTint="@color/white"
            android:text="Continuar"
            android:textColor="@color/black"
            android:layout_centerVertical="true"/>
    </RelativeLayout>






</RelativeLayout>